list1 = [ "dog" , "cat" , "bunny" ] 
list2 = [1,2,3,4,5]
list3= [True , False , True ] 
list4 = ["monkey" , 10 , True , "men"]
list1[1] = "mouse" 
list2.insert(2, 4.5)
list3.append(False)
list4.remove ("men")
print (list1) 
print (list2) 
print (list3) 
print (list4)
print ("-----------------------------------")
