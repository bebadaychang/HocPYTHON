list3 = ["Huynh", "Uyen", "Anh", "Ngan", "Ngoc", "Nhi", "Hang"]
for x in list3: 
    print (x)
print ("-----------------------------------")
for h in range (len(list3)):
  
    print (list3[h])
print ("-----------------------------------")
u=0
while u < len(list3):
    print (list3[u])
    u +=1
print ("-----------------------------------")
for i, x in enumerate (list3): 
    print (i, x)
print ("-----------------------------------")
fakelist3 =[]
for v in list3: 
    if "h" in v:
        fakelist3.append(v)
        print (fakelist3)
print ("-----------------------------------")
newlist3 = [j for j in list3 if "g" in j]
print (newlist3)